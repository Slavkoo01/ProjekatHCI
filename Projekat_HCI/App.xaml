<Application x:Class="Projekat_HCI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:customToolBar="clr-namespace:Projekat_HCI.CustomElements"
             xmlns:local="clr-namespace:Projekat_HCI"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/Brushes.xaml"/>
                <ResourceDictionary Source="Style/ToolBarStyle.xaml"/>
                <ResourceDictionary Source="Style/ComboBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/BorderStyle.xaml"/>
                <ResourceDictionary Source="Style/TextBlockStyle.xaml"/>
                <ResourceDictionary Source="Style/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Style/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/DataGridStyle.xaml"/>
                <ResourceDictionary Source="Style/GridStyle.xaml"/>
                <ResourceDictionary Source="Style/RichTextBox.xaml"/>
                <ResourceDictionary Source="Style/CheckBoxStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
        
    </Application.Resources>
</Application>
